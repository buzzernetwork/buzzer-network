{
  "name": "@buzzer-network/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:e2e": "tsx src/test/e2e.test.ts",
    "test:flow": "tsx src/test/core-flow.test.ts",
    "test:redis": "tsx src/test/redis-test.ts",
    "test:watch": "jest --watch",
    "migrate": "tsx src/db/migrate.ts",
    "migrate:rollback": "tsx -e \"import('knex').then(k => k.default(require('./src/db/knexfile').default[process.env.NODE_ENV || 'development']).migrate.rollback().then(() => process.exit(0)))\"",
    "migrate:latest": "tsx src/db/migrate.ts",
    "settlement": "tsx src/scripts/settlement.ts",
    "settlement:date": "tsx src/scripts/settlement.ts"
  },
  "dependencies": {
    "@maxmind/geoip2-node": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "fastify": "^4.24.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "maxmind": "^5.0.1",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.3",
    "winston": "^3.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "axios": "^1.13.1",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  }
}
